      %header
        %h2.blog-title
          %a{:href=>"blog", :rel=>"bookmark", :title=>" LINK to POST TITLE"} Blog Post Title Here
        %img.center{:src => "assets/images/header_logo.png", :alt => ""}
        = image_tag "header_Logo.png", :class => "txt-center"

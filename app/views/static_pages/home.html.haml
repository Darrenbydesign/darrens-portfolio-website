#carousel 
  %ul 
    - @images.each do |i|
      %li
        = link_to(image_tag("#{i}", :alt => "#{i}".sub(/^[^\/]+\//,"")), portfolio_path)
%section#RecentPosts
  %h2 Recent Posts
  .blk-desktop-8.blk-desktop-offset-2
